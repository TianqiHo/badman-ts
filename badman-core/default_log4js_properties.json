{
  "appenders": {
    "stdout_appender": {
      "type": "stdout",
      "layout": { "type": "pattern","pattern":"%[[DPID(%z)]-[%d{yyyy-MM-dd hh:mm:ss.SSS} %c-%p] [%f{1}<%A>.%M(%l)] %] <=> %m %n" }
    }
  },
  "categories": {
    "default": {
      "appenders": ["stdout_appender"],
      "level": "info",
      "enableCallStack": true
    }
  }
}
